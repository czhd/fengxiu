<view class="container">
	<view class="title">
		<text >{{fence.title}}</text>
	</view>
	<view class="row-container">
		<block wx:for="{{fence.cells}}" wx:key="index">
			<s-cell class="cell" x="{{x}}" y="{{index}}" cell="{{item}}"></s-cell>
		</block>
		<view class="hr"></view>
	</view>
	<!-- <view class="hr"></view> -->
</view>
