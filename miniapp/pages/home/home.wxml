<view class="container">
    <image class="top-theme" src="{{themeA.entrance_img}}"></image>
	<swiper class="swiper" indicator-dots indicator-active-color="#157658" autoplay="{{true}}" circular>
		<block wx:key="index" wx:for="{{bannerB.items}}">
			<swiper-item class="" item-id="">
				<image class="swiper" src="{{item.img}}" />
			</swiper-item>
		</block>
	</swiper>	
	<s-category-grid grid="{{grid}}"></s-category-grid>
	<image class="activity" src="{{activity.entrance_img}}" ></image>
	<s-spu-scroll theme="{{themeSpu}}" wx:if="{{themeSpu.online === true}}"></s-spu-scroll>
	<image class="quality" src="{{themeF.entrance_img}}" ></image>
</view>
  
